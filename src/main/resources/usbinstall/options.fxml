<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="usbinstall.OptionsController">
<children>
      <GridPane hgap="5.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" />
          <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
          <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Error upon component installation:" GridPane.rowIndex="1">
<padding>
<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
</padding>
            </Label>
            <ComboBox fx:id="componentInstallError" onAction="#onComponentInstallError" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Installation log threshold:">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
            <ComboBox fx:id="logInstallThreshold" onAction="#onLogInstallThreshold" GridPane.columnIndex="1" />
         </children>
      </GridPane><GridPane VBox.vgrow="ALWAYS">
  <columnConstraints>
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="BOTTOM" vgrow="SOMETIMES" />
  </rowConstraints>
<children><Button mnemonicParsing="false" onAction="#onReset" text="Reset" /><Button fx:id="clearButton" mnemonicParsing="false" onAction="#onClear" text="Clear" GridPane.columnIndex="1" />
            <Button mnemonicParsing="false" onAction="#onDone" text="Done" GridPane.columnIndex="2" />
</children>
<padding>
<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
</padding>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
</GridPane>
</children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding></VBox>
